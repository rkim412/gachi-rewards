{
  "buildCommand": "npm run build && npm run setup:prod",
  "installCommand": "npm install",
  "framework": null,
  "outputDirectory": "build/client",
  "devCommand": null,
  "regions": ["iad1"],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/api/index"
    }
  ],
  "functions": {
    "api/index.js": {
      "runtime": "nodejs20.x",
      "maxDuration": 30
    }
  }
}
